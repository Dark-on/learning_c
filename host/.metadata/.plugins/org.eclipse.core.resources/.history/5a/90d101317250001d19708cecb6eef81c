#include <stdio.h>


int main(void)
{
	float num1, num2, num3;
	printf("Enter first number: ");
	scanf("%f", &num1);
	printf("Enter second number: ");
	scanf("%f", &num2);
	printf("Enter third number: ");
	scanf("%f", &num3);
	printf("Average: %0.2f", (num1 + num2 + num3) / 3);

	return 0;

}
